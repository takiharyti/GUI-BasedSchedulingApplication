<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="440.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AddCustomer">
    <Button fx:id="addButton" layoutX="14.0" layoutY="404.0" mnemonicParsing="false" onAction="#addButtonClicked" text="Add" />
    <Button fx:id="cancelButton" layoutX="834.0" layoutY="404.0" mnemonicParsing="false" onAction="#cancelButtonClicked" text="Cancel" />
    <Label layoutX="314.0" layoutY="131.0" text="Customer ID" />
    <Label layoutX="314.0" layoutY="158.0" text="Customer Name" />
    <Label layoutX="314.0" layoutY="185.0" text="Address" />
    <Label layoutX="314.0" layoutY="212.0" text="Postal Code" />
    <Label layoutX="314.0" layoutY="239.0" text="Phone" />
    <Label layoutX="314.0" layoutY="266.0" text="Country" />
    <Label layoutX="314.0" layoutY="293.0" text="Division" />
    <TextField fx:id="customerId" disable="true" layoutX="437.0" layoutY="127.0" />
    <TextField fx:id="customerNameText" layoutX="437.0" layoutY="154.0" />
    <TextField fx:id="addressText" layoutX="437.0" layoutY="181.0" />
    <TextField fx:id="postalText" layoutX="437.0" layoutY="208.0" />
    <TextField fx:id="phoneText" layoutX="437.0" layoutY="235.0" />
    <ComboBox fx:id="countryCombo" layoutX="437.0" layoutY="262.0" onAction="#conComboBoxPicked" prefWidth="150.0" />
    <ComboBox fx:id="divisionCombo" layoutX="437.0" layoutY="289.0" prefWidth="150.0" />
</AnchorPane>
