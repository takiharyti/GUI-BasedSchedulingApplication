<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="440.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Main">
   <TableView fx:id="tV" layoutX="14.0" layoutY="14.0" prefHeight="379.0" prefWidth="874.0" />
   <Button fx:id="addButton" layoutX="14.0" layoutY="404.0" mnemonicParsing="false" onAction="#addButtonClicked" text="Add" />
   <Button fx:id="updateButton" layoutX="57.0" layoutY="404.0" mnemonicParsing="false" onAction="#updateButtonClicked" text="Update" />
   <Button fx:id="deleteButton" layoutX="117.0" layoutY="404.0" mnemonicParsing="false" onAction="#deleteButtonClicked" text="Delete" />
   <Button fx:id="appointmentsButton" layoutX="413.0" layoutY="404.0" mnemonicParsing="false" onAction="#appointmentsButtonClicked" text="Appointments" />
   <Button fx:id="customersButton" disable="true" layoutX="335.0" layoutY="404.0" mnemonicParsing="false" onAction="#customersButtonClicked" text="Customers" />
   <RadioButton fx:id="monthlyFilterRadio" disable="true" layoutX="566.0" layoutY="408.0" mnemonicParsing="false" onAction="#monthlyFilterRadioClicked" text="Monthly">
      <toggleGroup>
         <ToggleGroup fx:id="filter" />
      </toggleGroup>
   </RadioButton>
   <RadioButton fx:id="noneFilterRadio" disable="true" layoutX="510.0" layoutY="408.0" mnemonicParsing="false" onAction="#noneFilterRadioClicked" selected="true" text="None" toggleGroup="$filter" />
   <RadioButton fx:id="weeklyFilterRadio" disable="true" layoutX="637.0" layoutY="408.0" mnemonicParsing="false" onAction="#weeklyFilterRadioClicked" text="Weekly">
      <toggleGroup>
         <ToggleGroup fx:id="filter1" />
      </toggleGroup>
   </RadioButton>
   <Button fx:id="nextButton" disable="true" layoutX="845.0" layoutY="404.0" mnemonicParsing="false" onAction="#nextButtonClicked" text="Next" />
   <Button fx:id="backButton" disable="true" layoutX="799.0" layoutY="404.0" mnemonicParsing="false" onAction="#backButtonClicked" text="Back" />
   <Button fx:id="reportsButton" layoutX="173.0" layoutY="404.0" mnemonicParsing="false" onAction="#reportsButtonClicked" text="Reports" />
</AnchorPane>
